<div class="page-header">
  <div class="container-fluid">
    <span class="pull-left">
    <span><img src=<%= user[:image] %> alt="avatar" class="img-thumbnail" style="width:150px;height:150px"></span>
      <h3><%= user['name'] %></h3>
      <h5><%= user['email'] %></h5>
    </span>
  </div>
</div>

<div id="followee-feed">
  Tweets
  <div class="table-responsive">
    <table class="table table-striped table-hover">
      <tr><td class="text-center">Getting the users you are following. Please wait...</td></tr>
    </table>
  </div>
</div>

<%= erb :feed_followees, :locals => {users: users } %>